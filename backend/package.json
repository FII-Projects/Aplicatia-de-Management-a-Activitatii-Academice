    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.34",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.8.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^8.5.6",
